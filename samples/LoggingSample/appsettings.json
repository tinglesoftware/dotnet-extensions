{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "cli",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "IncludeCategory": false,
        "IncludeEventId": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
      }
    }
  }
}
